<%- include('header'); -%>
    <h1> <%= title %>
    </h1>
    <ui>
        <% for(var i=0; i<newitem.length; i++){ %>
             <li> <%= newitem[i] %> </li>
             <% }%>
    </ui>
    <form action="/" method="post">
        <div class="form-floating">
            <input type="text" class="form-control" id="floatingInput" name="item">
            <button class="w-100 btn btn-lg btn-primary" type="submit" name="list"  value= <%= title %> >Add items</button>
        </div>
    </form>
</body>
<%- include('footer'); -%>
</html>